<div id="dashboard-container">
  <h2>Community Questions</h2>
</div>
  {{#each questions}}
  <div id="questions-container">
    {{#with user_id}}
      <img src="{{ this.avatarUrl }}" class="avatar-question"/>
      <h4>{{this.firstName}}</h4>
    {{/with}}
    <h4>{{ this.industry }}</h4>
    <p><a href="community/question/{{this._id}}"> "{{ this.question }}?" </a></p>
    {{!-- <p>{{user_id.firstName}}</p> --}}
    <div id="stats-container">
      <table class="">
        <thead>
          <tr class="stats-items">
            <th align="" class="">Answers</th>
            <th align="" class="">Likes</th>
            <th align="" class="">Status</th>
            <th align="" class="">Flag</th>
          </tr>
        </thead>
        <tbody>
          <tr class="stats-items">
            <td align="" class=""> {{ this.answers }} </td>
            <td align="" class=""> {{ this.likes }} </td>
            <td align="" class=""> {{ this.status }}</td>
            <td align="" class=""> &#9872 </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  {{/each}}