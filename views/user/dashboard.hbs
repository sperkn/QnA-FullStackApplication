  <section id="questions">
    <h2>My Questions</h2>
    <table class="table table-striped table-hover table-condensed">
      <thead>
        <tr>
          <th class="">Question</th>
          <th class="">Industry</th>
          <th class="">Answers</th>
          <th class="">Likes</th>
          <th class="">Status</th>
        </tr>
      </thead>
      <tbody>
        {{#each questions}}
          <tr>
            <td align="left" class=""> <a href="post/{{this._id}}"> {{ this.question }} </a></td>
            <td align="center" class=""> {{ this.industry }} </td>
            <td align="center" class=""> {{ this.answers }} </td>
            <td align="center" class=""> {{ this.likes }} </td>
            <td align="center" class=""> {{ this.status }}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </section>