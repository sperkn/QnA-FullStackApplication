  <section id="questions">
    <h2>My Questions</h2>
    <table class="table table-striped table-hover table-condensed">
      <thead>
        <tr>
          <th class="text-center">Question</th>
          <th class="text-center">Industry</th>
          <th class="text-center">Answers</th>
          <th class="text-center">Likes</th>
          <th class="text-center">Status</th>
        </tr>
      </thead>=
      <tbody>
        {{#each questions}}
          <tr>
            <td align="left" class="text-center"> <a href="post/{{this._id}}"> {{ this.question }} </a></td>
            <td align="center" class="text-center"> {{ this.industry }} </td>
            <td align="center" class="text-center"> {{ this.answers }} </td>
            <td align="center" class="text-center"> {{ this.likes }} </td>
            <td align="center" class="text-center"> {{ this.status }}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </section>